package main.java.com.dreamatn;

/**
 * 阿姆斯壮数 描述 ： 在三位的整数中，例如153可以满足1*1*1 + 5*5*5 + 3*3*3 = 153，这样的数称之为Armstrong数
 * 打印三位数中所有的armstrong数
 */
public class Armstrong {
	public static void main(String[] args) {
		for (int i = 100; i < 1000; i++) {
			int a, b, c;
			a = i / 100;
			b = (i % 100) / 10;
			c = (i % 100) % 10;
			if (a * a * a + b * b * b + c * c * c == i) {
				System.out.print(i + " ");
			}
		}

	}
}
