package main.java.com.dreamatn;

/**
 * 阿姆斯壮数
 * 描述 ： 在三位的整数中，例如153可以满足1^3 + 53 + 33 = 153，这样的数称之为Armstrong数
 */
public class Armstrong {
	public static void main(String[] args) {
		int num = 153;
		int a,b,c;
		a = num/100;
		b = (num%100)/10;
		c = (num%100)%10;
		System.out.println("a" + a + "b" + b + "c" + c);
		
	}
}
